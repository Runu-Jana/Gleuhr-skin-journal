{
  "name": "gleuhr-skin-journal",
  "version": "1.0.0",
  "description": "Gleuhr Skin Journal - React + Node.js PWA",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "install-all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "axios": "^1.13.5",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^9.2.2",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "gleuhr",
    "skincare",
    "journal",
    "pwa",
    "react",
    "nodejs"
  ],
  "author": "Gleuhr",
  "license": "MIT"
}
